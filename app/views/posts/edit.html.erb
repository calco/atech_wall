<% title "Edit Post" %>

<%= form_for @post do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :content %><br />
    <%= f.text_area :content %>
  </p>
  <p>
  </p>
  <p><%= f.submit %></p>
<% end %>


<p>
  <%= link_to "Show", @post %> |
  <%= link_to "View All", posts_path %>
</p>
